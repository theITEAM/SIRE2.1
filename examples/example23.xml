<observation tmin="0" tmax="infinity"/>

<!--Model priors-->
<prior parameter="beta" type="Flat" val1="0" val2="1"/>
<prior parameter="latent_period" type="Flat" val1="0" val2="10"/>
<prior parameter="detection_period" type="Flat" val1="0" val2="10"/>
<prior parameter="recovery_period" type="Flat" val1="0" val2="10"/>

<prior parameter="donor_inf" type="Flat" val1="-0.5" val2="0.5"/>
